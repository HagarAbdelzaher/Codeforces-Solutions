#define  FOR(i,n) for(int i=0;i<n;i++)
#define rFOR(i,x,n) for(int i=x;i>n;i--)
#define SZ(x) (int)x.size()
#define pb push_back
#define fi first
#define se second
typedef long long ll;
#include <stdio.h>
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void run() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr), cout.tie(nullptr);
}
int main()
{
run();
int n;
cin>>n;
string s;
cin>>s;
if(s.size()==1)
{
    cout<<"NO\n";
    exit(0);
}
for(int i=0 ; i<n-1 ; i++)
{
    if(s[i]!=s[i+1])
    {
        cout<<"YES\n";
        cout<<s[i]<<s[i+1];
        exit(0);
    }
}
cout<<"NO\n";
}
